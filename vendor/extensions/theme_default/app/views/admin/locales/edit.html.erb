<%= render :partial => 'admin/shared/configuration_menu' %>

<h1><%= t("editing_locale") %></h1>
<%= error_messages_for :locale %>
<% form_for(@locale, :url => object_url, :html => { :method => :put }) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%=submit_tag t("update")%>
  </p>
<% end %>